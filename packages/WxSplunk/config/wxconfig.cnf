wx.config.env.PROD=../../../config/packages/WxSplunk/wxconfig-PROD.cnf
wx.config.env.QS=../../../config/packages/WxSplunk/wxconfig-QS.cnf
wx.config.env.TEST=../../../config/packages/WxSplunk/wxconfig-TEST.cnf
wx.config.env.DEV=../../../config/packages/WxSplunk/wxconfig-DEV.cnf
wx.config.env.LOCAL-DEV=../../../config/packages/WxSplunk/wxconfig-LOCAL-DEV.cnf

# Count of retries for sending messages to Splunk
splunk.logging.maxDeliveryAttempts=15
# Count of messages which can be stored in the internal queue and waiting for sending
splunk.logging.maxMessagesInQueue=10000

# How many events shall be written to Splunk in one call
splunk.logging.batchSize=500
# How often should wx.splunk.impl:continuousSplunkLoggerThread become active:
splunk.logging.minSleepingTimeAfterBatchInMilliseconds=500
splunk.logging.maxSleepingTimeAfterBatchInMilliseconds=2000

# For debugging purpose
splunk.logging.tracingEnabled=false
